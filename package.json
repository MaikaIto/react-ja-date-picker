{
  "name": "calender-picker",
  "version": "0.0.0",
  "description": "カレンダーピッカーのReactコンポーネント",
  "main": "lib/CalenderPicker.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build:dev": "browserify ./preview/main.jsx -t babelify -o ./preview/bundle.js",
    "build": "NODE_ENV=production babel ./src --out-dir lib --source-maps inline && npm run build:dev",
    "start": "npm run build:dev && http-server ./preview -io -p 8080",
    "lint": "eslint .",
    "test": "npm run lint && mocha"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {}
}
